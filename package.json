  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/lodash": "^4.14.137",
    "@types/marked": "^0.6.5",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "@types/react-transition-group": "^4.2.2",
    "babel-eslint": "^9.0.0",
